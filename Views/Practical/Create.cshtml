@model ChemisTrackCrud.Models.PracticalModel

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>PracticalModel</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.PracticalName)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.PracticalName)
            @Html.ValidationMessageFor(model => model.PracticalName)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.PracticalYear)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.PracticalYear)
            @Html.ValidationMessageFor(model => model.PracticalYear)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ChemicalID, "Chemicals")
        </div>
        <div class="editor-field">
            @Html.DropDownList("ChemicalID", String.Empty)
            @Html.ValidationMessageFor(model => model.ChemicalID)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.EquipmentID, "Equipments")
        </div>
        <div class="editor-field">
            @Html.DropDownList("EquipmentID", String.Empty)
            @Html.ValidationMessageFor(model => model.EquipmentID)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
