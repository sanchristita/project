@model ChemisTrackCrud.Models.UsedListsModel

@{
    ViewBag.Title = "Details";
}

<h2 class="item-title"><span class="icon-chemical padd-right"></span>Chemical Used List Details</h2>

    <div class="form-group clearfix">
        <div class="col-sm-4 text-right">@Html.DisplayNameFor(model => model.UsedDate) :</div>
        <div class="col-sm-6">@Html.DisplayFor(model => model.UsedDate)</div>
    </div>

    <div class="form-group clearfix">
        <div class="col-sm-4 text-right">@Html.DisplayNameFor(model => model.PracticalNo) :</div>
        <div class="col-sm-6">@Html.DisplayFor(model => model.PracticalNo)</div>
    </div>

    <div class="form-group clearfix">
        <div class="col-sm-4 text-right">@Html.DisplayNameFor(model => model.YearOfCourse) :</div>
        <div class="col-sm-6">@Html.DisplayFor(model => model.YearOfCourse)</div>
    </div>

    <div class="form-group clearfix">
        <div class="col-sm-4 text-right">@Html.DisplayNameFor(model => model.Chemicals.ChemicalName) :</div>
        <div class="col-sm-6">@Html.DisplayFor(model => model.Chemicals.ChemicalName)</div>
    </div>

    <div class="form-group clearfix">
        <div class="col-sm-4 text-right">@Html.DisplayNameFor(model => model.UsedQuantity) :</div>
        <div class="col-sm-6">@Html.DisplayFor(model => model.UsedQuantity)</div>
    </div>

    <div class="form-group clearfix">
        <div class="col-sm-4 text-right">@Html.DisplayNameFor(model => model.ChemicalStockCount) :</div>
        <div class="col-sm-6">@Html.DisplayFor(model => model.ChemicalStockCount)</div>
    </div>

<p class="col-sm-10 text-center">
    @Html.ActionLink("Edit", "Edit", new { id=Model.UsedListsID }) |
    @Html.ActionLink("Back to List", "Index", null, new { @class="btn btn-default"})
</p>
