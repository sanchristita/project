@model ChemisTrackCrud.Models.UsedListsModel

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>UsedListsModel</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.UsedDate)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.UsedDate)
            @Html.ValidationMessageFor(model => model.UsedDate)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.PracticalNo)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.PracticalNo)
            @Html.ValidationMessageFor(model => model.PracticalNo)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.YearOfCourse)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.YearOfCourse)
            @Html.ValidationMessageFor(model => model.YearOfCourse)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ChemicalID, "Chemicals")
        </div>
        <div class="editor-field">
            @Html.DropDownList("ChemicalID", String.Empty)
            @Html.ValidationMessageFor(model => model.ChemicalID)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.UsedQuantity)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.UsedQuantity)
            @Html.ValidationMessageFor(model => model.UsedQuantity)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ChemicalStockCount)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ChemicalStockCount)
            @Html.ValidationMessageFor(model => model.ChemicalStockCount)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
