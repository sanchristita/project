@model ChemisTrackCrud.Models.StudentsModel

@{
    ViewBag.Title = "Create";
}

<h2 class="item-title"><span class="icon-student padd-right"></span>Create New Defect Chemical </h2>

@using (Html.BeginForm("Create","Student",FormMethod.Post)) {
    @Html.ValidationSummary(true)

   
        <div class="form-group clearfix">
            @Html.LabelFor(model => model.StudentName, new {@class = "col-sm-4 text-right" })
            @Html.EditorFor(model => model.StudentName, new {@class = "col-sm-6 form-control" })
            @Html.ValidationMessageFor(model => model.StudentName)
        </div>

        <div class="form-group clearfix">
            @Html.LabelFor(model => model.Year, new {@class = "col-sm-4 text-right" })
            @Html.EditorFor(model => model.Year, new {@class = "col-sm-6 form-control" })
            @Html.ValidationMessageFor(model => model.Year)
        </div>

        <p class="col-sm-10 text-center">
            <input type="submit" value="Create" class="btn btn-success"  />
            @Html.ActionLink("Back to List", "Index", null, new { @class="btn btn-default"})
        </p>
   
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
